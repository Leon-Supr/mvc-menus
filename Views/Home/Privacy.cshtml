@{
    Layout = "";
    ViewData["Title"] = "Privacy Policy";
}


<html>

<head>

    <meta charset="utf-8" />

    <title>@ViewData["Title"]</title>

    <script src="https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js"></script>

</head>

<body>
    <!-- Contenedor VUE -->
    <div id="app">
        <h2>{{ mensaje }}</h2>
        <p>Hola {{ nombre }}</p>
    </div>
    <!-- Scripts VUE -->
    <script>
        new Vue({
            el: '#app',
            data: {
                mensaje: 'Bienvenido!!!',
                nombre: 'Chaval'
            }
        });
    </script>
</body>

</html>